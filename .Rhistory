# Function to convert countdown timestamp to total seconds
convert_timestamp_to_seconds <- function(timestamp) {
# Split the timestamp by colon and dot
parts <- unlist(strsplit(timestamp, "[:.]"))
# Initialize time variables with default values
hours <- ifelse(length(parts) >= 4, as.numeric(parts[1]), 0)
minutes <- ifelse(length(parts) >= 3, as.numeric(parts[length(parts) - 2]), 0)
seconds <- ifelse(length(parts) >= 2, as.numeric(parts[length(parts) - 1]), 0)
tenths <- ifelse(length(parts) == 4, as.numeric(parts[4]) / 10, 0)  # Convert tenths to seconds
total_seconds <- (hours * 3600) + (minutes * 60) + seconds + tenths
return(total_seconds)
}
